    "@typescript-eslint/parser": "5.29.0",
    "archiver": "^7.0.1",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.1",
    "obsidian": "latest",
    "ts-jest": "^29.4.5",
    "tslib": "2.4.0",
